apiVersion: apps/v1
kind: Deployment
metadata:
  name: frontend-app
spec:
  replicas: 1
  selector:
    matchLabels:
      run: frontend-app
  template:
    metadata:
      labels:
        run: frontend-app
    spec:
      containers:
      - name: node-app
        image: palani160385/frontend-app
        imagePullPolicy: IfNotPresent
        ports:
        - containerPort: 80
        - containerPort: 443
      imagePullSecrets:
      - name: docker-secret-key
